      "google-analytics.com",
      "googletagmanager.com",
      "facebook.com/tr",
      "connect.facebook.net",
      "adservice.google.com",
      "pagead2.googlesyndication.com",
      "outbrain.com",
      "taboola.com",
      "advertising.com",
      "adsystem.com",
      "adnxs.com",
      "criteo.com",
      "pubmatic.com",
      "exoclick.com",
      "popads.net",
      "adcash.com",
      "propellerads.com"
    ],
    "urlPatterns": [
