        "[id*='google_ads']",
        "[id*='adunit']",
        "[class*='adsbygoogle']",
        "[id*='pop']",
        "[class*='pop']"
      ],
      "elements": [
        "link[rel='icon']",
        "link[rel='shortcut icon']",
        "iframe[src*='ads']",
        "iframe[src*='doubleclick']",
        "iframe[src*='google']",
        "a[target='_blank']"
      ]
    }
  }
}

